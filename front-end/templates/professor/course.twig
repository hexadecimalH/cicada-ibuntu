            advlist_bullet_styles: "square",
            setup : function(ed) {
                ed.on("change", function(e){
                    $('#content').html(tinymce.activeEditor.getContent());
                });
                ed.on("keyup", function(){
                    $('#content').html(tinymce.activeEditor.getContent());
