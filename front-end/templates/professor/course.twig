            <div class="blank">


                <div class="blank-page">
                    <h1>{{ course.course_name }}</h1>
                    <h3 class="sub-heading">{{ department.name }}</h3>
                    <tabs v-model="activeTab" nav-style="tabs" justified>
                        <tab header="Course Info">
                            <div class="col-sm-12 content-block" id="content">

                            </div>
                            <div class="col-sm-12" v-bind:class="{ 'not-shown': !editMode }">
                                <textarea name="" id="tinymce" cols="30" rows="10"></textarea>
                            </div>
                            <div class="col-sm-12">
                                <p class="button-holder">
                                    <button class="btn btn-default"  v-on:click="toggleEdit" v-if="!editMode">Edit</button>
                                    <button class="btn btn-success"  v-on:click="sendCourseInfo" :data-id="courseId" v-if="editMode">Save Changes</button>
                                    <button class="btn btn-primary"  v-on:click="getCourseData" v-if="editMode">Undo Changes</button>
                                    <button class="btn btn-info"  v-on:click="toggleEdit" v-if="editMode">Cancel Edit</button>
                                </p>
                            </div>
                            <vue-event-calendar :events="demoEvents">
                                <template scope="props">
                                    <div v-for="(event, index) in props.showEvents" class="event-item">

                                        {{event.title}}
                                    </div>
                                </template>
                            </vue-event-calendar>

                            <div class="clearfix"></div>

                        </tab>
                        <tab header="Requests">
                            <table class="table">
                                <tbody>
                                <th>Picture</th>
                                <th>Student Info</th>
                                <th>Request Status</th>
                                <th>Action</th>
                                <tr class="table-row" v-for="request in requests">
                                    <td class="table-img">
                                        <img :src="request.users.picture" alt="" width="60px">
                                    </td>
                                    <td class="table-text">
                                        <h6>${request.users.first_name} ${request.users.last_name}</h6>
                                        <p>${request.users.email}</p>
                                    </td>
                                    <td class="table-text">
                                        <span class="mar" v-if="request.status == 'pending'"> ${request.status}</span>
                                        <span class="work" v-else-if="request.status =='approved'"> Approved </span>
                                    </td>

                                    <td>
                                        <button class="btn btn-success" v-if="request.status == 'pending'" v-on:click="acceptStudentRequest" :data-id="request.id">Accept Request</button>
                                        <button class="btn btn-danger" v-if="request.status == 'pending'" v-on:click="deleteStudentRequest" :data-id="request.id">Delete Request</button>
                                        <button class="btn btn-danger" v-if="request.status == 'approved'" v-on:click="deleteStudentRequest" :data-id="request.id">Remove Student</button>

                                    </td>
                                </tr>
                                </tbody>
                            </table>

                        </tab>
                        <tab header="Course Resources">
                            <h2>Upload files for {{ course.course_name }}</h2>
                            <h3 class="sub-heading">{{ department.name }}</h3>
                            <div class="col-sm-12">
                                <div class="col-sm-3 back-file " v-for="file in courseFiles">
                                    <div class="gallery-img" style="text-align: center">
                                        <a href="#" class="b-link-stripe b-animate-go  swipebox"  title="Image Title" >
                                            <i class="fa fa-files-o fa-4x" aria-hidden="true"></i>
                                        </a>
                                    </div>
                                    <div class="text-gallery gallery-file">
                                        <p class="align">${file.file_name}</p>

                                        <p class="align">
                                        <form :action="file.url" method="get" style="display:inline">
                                            <button class="btn-primary btn download-btn" type="submit">Download Files</button>
                                        </form>
                                        <button class="btn-danger btn download-btn" v-on:click="deleteStoredFiles(file.id)">Delete Files</button>
                                        </p>

                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12 content-block" id="files">
                                <h3 class="sub-heading">Upload new files for {{ course.course_name }}</h3>
                                <new-files></new-files>
                            </div>
                            <div class="clearfix"></div>
                        </tab>
                        <tab header="Course Assignments">
                            <div class="content-box assignments" v-for="assignmet in assignments">
                                <div class="col-sm-12 assignments">
                                    <div class="col-sm-10">
                                        <h3 v-text="assignmet.name"></h3>
                                        <p v-text="assignmet.description"></p>
                                        <form :action="assignmet.url" method="get" style="display:inline">
                                            <button class="btn btn-success" type="submit" >Get Files</button>
                                        </form>
                                    </div>
                                    <div class="col-sm-2">
                                        <h6>Due date</h6>
                                        <p v-text="beautifyDate(assignmet.due_date)"></p>
                                        <p>
                                            <button class="btn-danger btn" :data-id="assignmet.id" data-toggle="modal" data-target="#assignmentModal" v-on:click="editAssignment">Edit</button>
                                            <button class="btn-danger btn" :data-id="assignmet.id" v-on:click="deleteAssignment">Delete</button>
                                        </p>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="content-box assignments">
                                <new-assignment :ismodal="false"></new-assignment>
                                <div class="clearfix"></div>
                            </div>
                            <div class="modal fade" id="assignmentModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" >
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                                            <h2 class="modal-title">Edit Assignment</h2>
                                        </div>

                                        <div class="modal-body">
                                            <new-assignment :ismodal="true" v-bind:assignment="currentAssignment"></new-assignment>
                                            <div class="clearfix"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </tab>
                    </tabs>
                </div>

